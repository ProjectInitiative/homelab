{
  "address": ":9000",
  "dnsNames": [
    "step-ca.step-ca.svc.cluster.local",
    "step-ca.step-ca.svc"
  ],
  "logger": {
    "format": "json"
  },
  "db": {
    "type": "postgresql"
  },
  "ca": {
    "db": {
      "enabled": false,
      "persistent": false
    }
  },
  "authority": {},
  "tls": {
    "cipherSuites": [
      "TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384",
      "TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256"
    ],
    "minVersion": "1.2"
  },
  "acme": {
    "enabled": true
  }
}
